Procedure UpdateLife;
Var
 I:Integer;
Begin
 For I:=1 to 5 do Begin
  Life(I<=LifeCount,I);
 End;
End;

Procedure UpdateScore(AddScore:Integer);
Var
 S:String;
Begin
 SetColor(Black);
 Str(Score,S);
 OutTextXY(485,15,S);
 Inc(Score,AddScore);
 Str(Score,S);
 SetColor(Yellow);
 OutTextXY(485,15,S);
End;

Procedure UpdateWave(AddWave:Integer);
Var
 S:String;
Begin
 SetColor(Black);
 Str(Score,S);
 OutTextXY(485,65,S);
 Inc(Wave,AddWave);
 Str(Wave,S);
 SetColor(Yellow);
 OutTextXY(485,65,S);
End;

Procedure InitGame;Begin
 Score:=0;
 Wave:=0;
 LifeCount:=3;
 FillChar(BabyI,SizeOf(BabyI),0);
 Flames(True);
 Building(True);
 Ambulance(True);
 SetColor(White);
 Line(0,295,479,295);
 UpdateLife;
 SetColor(Yellow);
 OutTextXY(480,0,'Pointage :');
 UpdateScore(0);
 OutTextXY(480,50,'Wague :');
 UpdateWave(0);
End;

Procedure DeadBaby;
Var
 I:Byte;
Begin
 If LifeCount=1 Then Begin
  SetColor(LightRed);
  OutTextXY(300,220,'Vous avez perdu !');
  ReadKey;
  Terminated:=True
 End
  Else
 Begin
  Dec(LifeCount);
  UpdateLife;
  For I:=0 to High(BabyI)do Baby_(BabyI[I],False);
  FillChar(BabyI,SizeOf(BabyI),0);
  SetColor(LightRed);
  OutTextXY(300,220,'Vous avez perdu un bb !');
  ReadKey;
  SetColor(Black);
  OutTextXY(300,220,'Vous avez perdu un bb !');
  SetColor(White);
 End;
End;

Procedure Play;
Var
 Restart,AlreadyBaby:Boolean;
 FirePlace:Byte;
 I,J,Wait,WaveCount:Integer;
Begin
 Terminated:=False;
 WaveCount:=26;
 Wait:=0;
 FirePlace:=0;
 Firefighter1(True,FirePlace);
 Repeat
  Repeat
   For J:=High(BabyI) downto 0 do Begin
    If BabyI[J]=0 Then Begin
     If Wait<=0 Then Begin
      AlreadyBaby:=False;
      For I:=0 to High(BabyI)do If BabyI[I]=1 Then AlreadyBaby:=True;
      If Not(AlreadyBaby)Then Begin
       Baby_(BabyI[J],False);
       Inc(BabyI[J]);
       Baby_(BabyI[J],True);
      End;
      Wait:=Random(Abs(2000-Wave*200));
     End
      Else
     Dec(Wait);
    End
     Else
    If BabyI[J]<>255 Then Begin
     Baby_(BabyI[J],False);
     Restart:=False;
     Case BabyI[J] of
      8:If FirePlace<>0 Then Begin
       DeadBaby;
       Restart:=True;
      End;
      16:If FirePlace<>1 Then Begin
       DeadBaby;
       Restart:=True;
      End;
      22:If FirePlace<>2 Then Begin
       DeadBaby;
       Restart:=True;
      End;
     End;
     If(Terminated)Then Exit;
     If Not(Restart)Then Begin
      Inc(BabyI[J]);
      Baby_(BabyI[J],True);
     End;
    End;
   End;
   For I:=0 to High(BabyI)do If BabyI[I]=27 Then Begin
    UpdateScore(25);
    BabyI[I]:=255;
    Dec(WaveCount);
    If WaveCount=0 Then Begin
     UpdateWave(1);
     WaveCount:=26;
     FillChar(BabyI,SizeOf(BabyI),0);
     SetColor(LightGreen);
     OutTextXY(300,220,'Vague russie !');
     ReadKey;
     SetColor(Black);
     OutTextXY(300,220,'Vague russie !');
     SetColor(White);
    End;
   End;
   Delay(150);
  Until Keypressed;
  Case UpCase(ReadKey) of
   #0:Case ReadKey of
    #75:Begin { Gauche }
    Firefighter1(False,FirePlace);
    If FirePlace=0 Then FirePlace:=2
                   Else FirePlace:=FirePlace-1;
    Firefighter1(True,FirePlace);
   End;
    #77:Begin { Droite }
    Firefighter1(False,FirePlace);
    If FirePlace=2 Then FirePlace:=0
                   Else FirePlace:=FirePlace+1;
    Firefighter1(True,FirePlace);
   End;
   End;
   'A':Begin
    Firefighter1(False,FirePlace);
    FirePlace:=0;
    Firefighter1(True,FirePlace);
   End;
   'B':Begin
    Firefighter1(False,FirePlace);
    FirePlace:=1;
    Firefighter1(True,FirePlace);
   End;
   'C':Begin
    Firefighter1(False,FirePlace);
    FirePlace:=2;
    Firefighter1(True,FirePlace);
   End;
   #27:Terminated:=True;
  End;
 Until Terminated;
End;

BEGIN
 InitScr;
 InitGame;
 Play;
END.