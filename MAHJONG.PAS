Procedure DrawBoard;
Var
 i,j,z:Integer;
Begin
 ClearDevice;
 For z:=1 To BOARD_DEPTH Do
  For i:=1 To BOARD_WIDTH Do
   For j:=1 To BOARD_HEIGHT Do
    If Board[i,j,z].Active Then
     DrawTile(i,j,z, (i = CursorX) And (j = CursorY));
End;

Procedure ShowScore;
Var
 TempStr:String;
Begin
 SetFillStyle(SolidFill, Black);
 Bar(380, 10, 639, 100);  { Efface la zone de pointage }
 SetTextStyle(DefaultFont, HorizDir, 2);
 SetColor(White);
 Str(SolvedTiles, TempStr);
 OutTextXY(380, 20, 'Rsolues : ' + TempStr);
 Str(RemainingTiles, TempStr);
 OutTextXY(380, 50, 'Restantes : ' + TempStr);
End;

Procedure MoveCursor(Var x,y:Integer);
Var
 ch:Char;
 visibleZ:Integer;
Begin
 ch:=ReadKey;
 Case ch Of
  #27: QuitGame := True;  { Touche ESC }
  #0: Case ReadKey Of
   #72: If y > 1 Then Dec(y);           { Haut }
   #80: If y < BOARD_HEIGHT Then Inc(y); { Bas }
   #75: If x > 1 Then Dec(x);           { Gauche }
   #77: If x < BOARD_WIDTH Then Inc(x);  { Droite }
  End;
  #13: Begin { Enter }
   visibleZ := GetVisibleTileLevel(x, y);
   If(visibleZ > 0) And Not IsTileBlocked(x, y, visibleZ)Then Begin
    If Not FirstSelect Then Begin
     FirstX := x;
     FirstY := y;
     FirstZ := visibleZ;
     FirstSelect := True;
    End
     Else
    Begin
     If(Board[FirstX,FirstY,FirstZ].Value = Board[x,y,visibleZ].Value) And
        ((FirstX <> x) Or (FirstY <> y)) And
        Not IsTileBlocked(x, y, visibleZ) Then Begin
      Board[FirstX,FirstY,FirstZ].Active := False;
      Board[x,y,visibleZ].Active := False;
      Dec(RemainingTiles, 2);
      Inc(SolvedTiles, 2);
      ShowScore;  { Mise  jour du pointage }
     End;
     FirstSelect:=False;
    End;
   End;
  End;
 End;
End;

BEGIN
 InitializeGraphics;
 InitializeBoard;
 CursorX := 1;
 CursorY := 1;
 FirstSelect := False;
 QuitGame := False;  { Initialisation }
 SolvedTiles := 0;
 Repeat
  DrawBoard;
  ShowScore;  { Affichage initial du pointage }
  MoveCursor(CursorX, CursorY);
 Until (RemainingTiles = 0) Or QuitGame;
 CloseGraph;
 If RemainingTiles=0 Then WriteLn('Flicitations! Vous avez gagn!')
                     Else WriteLn('Jeu termin!');
END.
