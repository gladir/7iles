Procedure DrawTile(x,y,z:Integer;highlighted:Boolean);
Var
 baseX,baseY:Integer;
 points:Array[0..3] of TPoint;
Begin
 If Board[x,y,z].Active Then Begin
  baseX:=(x-1) * TILE_WIDTH + 10 + (z * 5);
  baseY:=(y-1) * TILE_HEIGHT + 10 - (z * 5);

    { Face suprieure 3D avec remplissage }
  Point(baseX, baseY,points[0]);
  Point(baseX + TILE_DEPTH, baseY - TILE_DEPTH,points[1]);
  Point(baseX + TILE_WIDTH + TILE_DEPTH, baseY - TILE_DEPTH,points[2]);
  Point(baseX + TILE_WIDTH, baseY,points[3]);

    { Face latrale 3D avec remplissage }
